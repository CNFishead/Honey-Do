    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.2",
    "slugify": "^1.6.4",
    "xss-clean": "^0.1.1"
